class person:
  def __init__(self,fname,lname):
    self.firstname=fname
    self.lastname=lname
  def printname(self):
    print(self.firstname,self.lastname)
class student(person):
  def __init__(self,fname,lname):
    person.__init__(self,fname,lname)
    self.sid=76
  def studID(self):
    print("student id is",self.sid)
s1=student("sara","fatima")
s1.printname()
s1.studID()
